			<Plasteel>15</Plasteel>
			<ComponentSpacer>4</ComponentSpacer>
		</costList>
		<butcherProducts>
			<Plasteel>15</Plasteel>
			<ComponentSpacer>4</ComponentSpacer>
		</butcherProducts>
		<recipeMaker>
			<researchPrerequisite>FSFResearchEnvironmentalImplants</researchPrerequisite>
