<?xml version="1.0" encoding="utf-8"?>

<Patch>
	
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			
			<li Class="PatchOperationTest">
				<xpath>/Defs/ThingDef[defName = "PsychicReader"]</xpath>
			</li>

	<!-- Add Royalty Research to Suppression Module -->
		
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName = "FSFImplantBrainSuppression"]</xpath>
				<value>
					<recipeMaker>
						<researchPrerequisite>BrainWiring</researchPrerequisite>
					</recipeMaker>
				</value>
			</li>
			
		</operations>
	</Operation>
	
</Patch>