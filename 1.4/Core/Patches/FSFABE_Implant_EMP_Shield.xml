<?xml version="1.0" encoding="utf-8"?>

<Patch>

	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
	
	<!-- Patch Brain Shock Hediff -->
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/HediffDef[defName = "BrainShock"]/comps</xpath>
				<value>
					<li Class="HediffCompProperties_RemoveIfOtherHediff">
						<otherHediff>FSFImplantEMPShield</otherHediff>
					</li>
				</value>
			</li>
	
		</operations>
	</Operation>
	
</Patch>